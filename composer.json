{
    "name": "steffenbrem/json-api-bundle",
    "description": "Integration of JSON API with Symfony2 using JMS Serializer.",
    "keywords": [
        "rest",
        "jsonapi",
        "json-api"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Brem",
            "email": "steffenbrem@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "symfony/yaml": "^2.3|^3.4",
        "symfony/property-access": "^2.3|^3.4",
        "symfony/validator": "^2.3|^3.4|^4.0",
        "symfony/expression-language": "^2.3|^3.4",
        "jms/serializer-bundle": "^0.13|^1.0|^2.0|^3.0",
        "pagerfanta/pagerfanta": "^2.0",
        "doctrine/common": "^2.4.8",
        "psr/log": "^1.0",
        "sensio/framework-extra-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-0": {
            "Mango\\Bundle\\JsonApiBundle": ""
        }
    },
    "target-dir": "Mango/Bundle/JsonApiBundle",
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
