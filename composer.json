{
    "name": "steffenbrem/json-api-bundle",
    "description": "Integration of JSON API with Symfony2 using JMS Serializer.",
    "keywords": [
        "rest",
        "jsonapi",
        "json-api"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Brem",
            "email": "steffenbrem@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/yaml": "^3.4|^4.0",
        "symfony/property-access": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "symfony/expression-language": "^3.4|^4.0",
        "jms/serializer-bundle": "^3.0",
        "pagerfanta/pagerfanta": "^2.0",
        "doctrine/common": "^2.4.8",
        "psr/log": "^1.0",
        "sensio/framework-extra-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "Mango\\Bundle\\JsonApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mango\\Bundle\\JsonApiBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
